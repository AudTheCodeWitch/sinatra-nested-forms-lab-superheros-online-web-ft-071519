<h1>Create a Team and Heroes!</h1>
<form action="/teams" method="post">
  <h2>Team</h2>
  <label for="team_name">Team Name:</label>
  <input id="team_name" type="text" name="team[name]"/><br>
  <label for="team_motto">Team Motto:</label>
  <input id="team_motto" type="text" name="team[motto]"/><br>

  <h3>Team Members</h3>
  <label for="member1_name">Hero 1 Name:</label><br>
  <input id="member1_name" type="text" name="team[heroes][][name]"/><br>
  <label for="member1_power">Hero 1 Power:</label><br>
  <input id="member1_power" type="text" name="team[heroes][][power]"/><br>
  <label for="member1_bio">Hero 1 Bio:</label><br>
  <input id="member1_bio" type="text" name="team[heroes][][bio]"/><br>

  <label for="member2_name">Hero 2 Name:</label><br>
  <input id="member2_name" type="text" name="team[heroes][][name]"/><br>
  <label for="member2_power">Hero 2 Power:</label><br>
  <input id="member2_power" type="text" name="team[heroes][][power]"/><br>
  <label for="member2_bio">Hero 2 Bio:</label><br>
  <input id="member2_bio" type="text" name="team[heroes][][bio]"/><br>

  <label for="member3_name">Hero 3 Name:</label><br>
  <input id="member3_name" type="text" name="team[heroes][][name]"/><br>
  <label for="member3_power">Hero 3 Power:</label><br>
  <input id="member3_power" type="text" name="team[heroes][][power]"/><br>
  <label for="member3_bio">Hero 3 Bio:</label><br>
  <input id="member3_bio" type="text" name="team[heroes][][bio]"/><br>

  <button type="submit" id="submit">Submit</button>
</form>